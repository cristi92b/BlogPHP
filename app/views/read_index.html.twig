<div id="posts">
    {% for key,post in posts %}
    <div class="post">
        <h2><a href="/read/{{post.id}}">{{post.title}}</a></h2>
        <h4>Created: {{post.createdTime}}</h4>
        <h4>Last Updated: {{post.updateTime}}</h4>
        <hr>
        {{post.content}}
    </div>
    {% endfor %}
</div>
<a href="/login">Login</a>
